<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Airplan.Controller_main_ticket">
   <children>
      <ImageView fitHeight="720.0" fitWidth="1280.0" pickOnBounds="true">
         <image>
            <Image url="@arkaplanlar4.jpg" />
         </image>
      </ImageView>
      <Button fx:id="book" layoutX="548.0" layoutY="160.0" mnemonicParsing="false" onAction="#ticket_start" prefHeight="129.0" prefWidth="194.0" text="Book Ticket">
         <font>
            <Font name="Alfios" size="29.0" />
         </font></Button>
      <Button fx:id="look" layoutX="549.0" layoutY="333.0" mnemonicParsing="false" onAction="#ticket_start" prefHeight="129.0" prefWidth="194.0" text="Look Passenger">
         <font>
            <Font name="Alfios" size="27.0" />
         </font></Button>
      <ImageView fx:id="back" fitHeight="74.0" fitWidth="82.0" layoutX="67.0" layoutY="29.0" onMouseClicked="#back" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@back.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
